#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>

int main(void) {
	
	int num1;
	int num2;
	
	printf("Digite o primeiro numero: ");
	scanf(" %d", &num1);
	
	printf("Digite o segundo numero: ");
	scanf(" %d", &num2);
	
	printf("Soma: %d\nSubtracao: %d\nMultiplicacao: %d\nDivisao: %d", num1 + num2, num1 - num2, num1 * num2, num1 / num2);
	
    return 0;
}
