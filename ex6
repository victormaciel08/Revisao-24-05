#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
#define X 10
int main(void) {
	
	int num[X];
	int i;
	int par = 0;
	int impar = 0;
	
	for(i = 0; i < X; i++){
		printf("Digite o numero %d: ", i+1);
		scanf(" %d", &num[i]);
	}

	for(i = 0; i < X; i++){
		if(num[i] % 2 == 0){
			par++;
		}
		else{
			impar++;
		}
	}
	
	printf("Pares: %d\nImpares: %d", par, impar);
    return 0;
}
